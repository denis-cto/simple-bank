{
  "compilerOptions": {
    "esModuleInterop": true,
    "noImplicitAny": false,
    "outDir": "dist",
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "pretty": true,
    "types": [
      "jest",
      "node"
    ],
    "sourceMap": true,
    "target": "es6",
    "baseUrl": "./"
  },
  "exclude": [
    "node_modules",
    "dist",
  ]
}
